{
    "name": "aw-studio/cms",
    "license": "MIT",
    "authors": [
        {
            "name": "jannescb",
            "email": "jannes@aw-studio.de"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "AwStudio\\Cms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "aw-studio/vitt": "^0.9|^0.10|^0.11",
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/support": "^8",
        "inertiajs/inertia-laravel": "^0.4|^0.5|^0.6",
        "litstack/litstack": "^3.7",
        "litstack/bladesmith": "^1.0",
        "litstack/pages": "^2.1",
        "litstack/meta": "^2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://store.litstack.io"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwStudio\\Cms\\CmsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
